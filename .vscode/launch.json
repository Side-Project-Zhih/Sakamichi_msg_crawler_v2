  "version": "0.2.0",
  "configurations": [
    {
      "name": "browser",
      "program": "${workspaceFolder}/index.js",
      // "request": "browser",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "msgCrawler_v2_nogi",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/app.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dst/**/*.js"],
      "args": ["-g", "nogi", "-m", "29", "40", "30"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "msgCrawler_v2_sakura",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/app.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dst/**/*.js"],
      "args": [
        "-g",
        "sakura",
        "-m",
        "21",
        "12",
        "15",
        "16",
        "33",
        "45",
        "47",
        "48",
        "51",
        "52",
        "56",
        "60",
        "7",
        "8"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "msgCrawler_v2_hinata",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/app.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dst/**/*.js"],
      "args": ["-g", "hinata", "-m", "36"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "look sakura members",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/app.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dst/**/*.js"],
      "args": ["-s"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "look nogi members",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/app.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dst/**/*.js"],
      "args": ["-n"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "look hinata members",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/app.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dst/**/*.js"],
      "args": ["-h"]
    }
  ]
}
